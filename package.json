{
  "name": "taotify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "check-prettier": "prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest --coverage --colors --config jest.config.js",
    "pre-commit": "npm run check-prettier && npm run check-lint && npm run check-types && npm run test && npx --no-install && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "coverage": "npx jest --coverage"
  },
  "engineStrict": true,
  "engines": {
    "node": ">14.0.0",
    "npm": ">=8.0.0",
    "yarn": "please-use-npm"
  },
  "dependencies": {
    "@antv/g": "^5.16.26",
    "@antv/g-canvas": "^1.9.46",
    "@antv/g-lite": "^1.0.66",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "msw": "^1.1.0",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-styling": "^0.3.2",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.2.7"
  }
